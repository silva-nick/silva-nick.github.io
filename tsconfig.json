{
    "compilerOptions": {
      "target": "es5",
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true, 
      "allowSyntheticDefaultImports": true,
      "strict": true, 
      "noEmit": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "incremental": true, 
      "noFallthroughCasesInSwitch": true
    },
    "include": [
      "scripts/*" 
    ],
    "exclude": ["phys121"] 
}
